generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model HumanContext {
  id                 String   @id @default(cuid())
  createdAt          DateTime @default(now())
  updatedAt          DateTime @updatedAt
  firstName          String
  lastName           String
  email              String
  country            String?
  city               String?
  publicAbout        String?
  privateAbout       String?
  privateMemories    Json?
  privatePreferences Json?
  secureInfo         Json?
  customRules        String?

  @@map("human_contexts")
}
